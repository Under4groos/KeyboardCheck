<Window x:Class="KeyboardCheck.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyboardCheck"
        mc:Ignorable="d" WindowState="Maximized" WindowStyle="None" AllowsTransparency="True"
        Title="MainWindow" Height="450" Width="800" ShowInTaskbar="False">
    <Window.Background>
        <SolidColorBrush></SolidColorBrush>
    </Window.Background>
    <Grid>
        <StackPanel Margin="10,0,0,0" x:Name="x_keys" Height="45" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Left">
            
        </StackPanel>
    </Grid>
</Window>
